<Window x:Class="PostImageSetup.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PostImageSetup"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
      <StackPanel  Orientation="Vertical">
      <Button Height="30" Width="100" Content="Baseline" Name="BaselineConfigButton" Click="BaselineConfigButton_Click" />
      <Button Height="30" Width="100" Content="Site" Name="SiteConfigButton" Click="SiteConfigButton_Click" />
      <Button Height="30" Width="100" Content="Installs" Name="InstallConfigButton" Click="InstallConfigButton_Click" />
      </StackPanel>
    </Grid>
</Window>
